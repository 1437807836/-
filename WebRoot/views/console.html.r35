    //导出CSV，即导出excel
    function exportCSV(item) {
        console.log('我进来了')
        if (item === 0) {
            $("#exportTips").show();
            $.ajax({
                url: '/yxbx/exportCSV',
                type: 'POST',
                data: {
                    'userId': userInfo.userId
                },
                success: function (res) {
                    var data = eval('(' + res + ')');
                    // console.log(data);
                    // alert(data.data);
                    window.location = "http://localhost:8082/yxbx/files/excel/" + data.data;
                    $("#exportTips").hide();
                    $("#waitDiv").hide();
                },
                error: function (res) {
                    console.log(res);
                    alert("请求失败！");
                }
            });
        } else if (item === 1) {
            if (checkitemIds.length === 0) {
                alert("请勾选清单前的复选框再操作！");
                return;
            }
            var idStr = "";
            for (var j = 0; j < checkitemIds.length; j++) {
                idStr = idStr + checkitemIds[j];
                if (j !== checkitemIds.length - 1) {
                    idStr = idStr + ",";
                }
            }
            $("#exportTips").show();
            $.ajax({
                url: '/yxbx/exportCSV',
                type: 'POST',
                data: {
                    'userId': userInfo.userId,
                    'idStr': idStr
                },
                success: function (res) {
                    var data = eval('(' + res + ')');
                    // console.log(data);
                    // alert(data.data);
                    window.location = "http://localhost:8082/yxbx/files/excel/" + data.data;
                    $("#exportTips").hide();
                    $("#waitDiv").hide();
                },
                error: function (res) {
                    alert("请求失败！");
                    console.log(res);
                }
            });
        } else {
            $("#waitDiv").hide();
        }

    }

